{
  "name": "@magicborn/dialogue-runner",
  "version": "0.1.0",
  "description": "Orchestration engine for running Yarn Spinner dialogues",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "yarn-spinner",
    "dialogue",
    "runner",
    "game-engine"
  ],
  "author": "MagicbornStudios",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MagicbornStudios/dialogue-runner.git"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}

